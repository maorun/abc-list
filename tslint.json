{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "max-line-length": [
      true,
      160
    ],
    "prefer-const": true,
    "object-literal-key-quotes": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "interface-name": false,
    "array-type": false,
    "eofline": false
  }
}
